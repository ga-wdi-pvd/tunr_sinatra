<h2><a href="/artists">All Artists</a></h2>
<div class="artist">
  <p>Artist Name: <%= @artists.name %></p>
  <p>Artist Nationality: <%= @artists.nationality %></p>
  <img src="<%= @artists.photo_url %>" alt="Photo" class="artist-photo">
  <br>

  <a href='/artists/<%= @artists.id %>/edit'>Edit This Artist</a><br>
  <a href='/artists/<%= @artists.id %>/delete'>Delete This Artist</a>

  <h2>The Artist's Songs (<%= @songs.count %>)</h2>
  <% @songs.each do |song|%>
  <p class="song">
    <a href="/songs/<%= song.id %>"><%= song.title %></a>
  </p>
  <% end %>





</div>
